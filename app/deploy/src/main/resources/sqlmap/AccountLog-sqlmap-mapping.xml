				FROM hqyt_account_log l
				LEFT JOIN hqyt_account a ON a.ACCOUNT_NO = l.ACCOUNT_NO 
				LEFT JOIN hqyt_user u ON u.USER_ID = a.LINK_ID 
		        WHERE 1 = 1 
		        <!-- AND (l.USER_ID IS NULL OR l.USER_ID="") -->
