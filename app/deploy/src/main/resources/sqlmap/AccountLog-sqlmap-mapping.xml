				<isNotEmpty prepend="and" property="cell"> 
					u.CELL like '%$cell$%'
				</isNotEmpty>
				<isNotEmpty prepend="and" property="nickName"> 
					u.NICK_NAME like '%$nickName$%'
				</isNotEmpty>
				<isNotEmpty prepend="and" property="logType"> 
					l.TYPE = #logType#
				</isNotEmpty>
				<isNotEmpty prepend="and" property="logFlg"> 
					l.FLG = #logFlg#
				</isNotEmpty>
				<isNotEmpty property="startDate" prepend="and">
